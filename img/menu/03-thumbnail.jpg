    <form onsubmit="sendTo(this.form);return false;">
  <script language="JavaScript">
function sendTo (form) { 
	if(window.location.href.indexOf("vietnamesische") > -1) {
    window.open ("https://www.lieferando.de/" + form.postcode.value + "#Cuisine=261");
	return false;
	} 
	else if(window.location.href.indexOf("thailaendische") > -1) {
    window.open ("https://www.lieferando.de/" + form.postcode.value + "#Cuisine=61");
	return false;
	} 
	else if(window.location.href.indexOf("chinesische") > -1) {
    window.open ("https://www.lieferando.de/" + form.postcode.value + "#Cuisine=41");
	return false;
	} 
	else if(window.location.href.indexOf("indische") > -1) {
    window.open ("https://www.lieferando.de/" + form.postcode.value + "#Cuisine=71");
	return false;
	} 
	else if(window.location.href.indexOf("japanische") > -1) {
    window.open ("https://www.lieferando.de/" + form.postcode.value + "#Cuisine=51,551");
	return false;
    } 
    else if(window.location.href.indexOf("spanische") > -1) {
    window.open ("https://www.lieferando.de/" + form.postcode.value + "#Cuisine=11");
    return false;
    } 
    else if(window.location.href.indexOf("griechische") > -1) {
    window.open ("https://www.lieferando.de/" + form.postcode.value + "#Cuisine=501,1356");
    return false;
    } 
    else if(window.location.href.indexOf("tuerkische") > -1) {
    window.open ("https://www.lieferando.de/" + form.postcode.value + "#Cuisine=511,821,291");
    return false;
    } 
    else if(window.location.href.indexOf("italienische") > -1) {
    window.open ("https://www.lieferando.de/" + form.postcode.value + "#Cuisine=271,21,1614");
    return false;
    } 
    else if(window.location.href.indexOf("mexikanische") > -1) {
    window.open ("https://www.lieferando.de/" + form.postcode.value + "#Cuisine=221");
    return false;
    } 
	else {
    window.open ("https://www.lieferando.de/" + form.postcode.value);
	return false;
}
}
$(window).load(function(){
$("input#inputfield").on({
  keydown: function(e) {
    if (e.which === 32)
      return false;
  },
  change: function() {
    this.value = this.value.replace(/\s/g, "");
  }
});
});
	</script>
